<h1>Listing results</h1>

<table>
  <tr>
    <th>Match</th>
    <th>User</th>
    <th>Score</th>
    <th>Result</th>
    <th>Active</th>
    <th>Points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @results.each do |result| %>
  <tr>
    <td><%= result.match_id %></td>

  <% user = user_name(result.user_id) %>
  <td><%= user.name %></td>


   
    <td><%= result.score %></td>
    <td><%= result.result %></td>
    <td><%= result.active %></td>
    <td><%= result.points %></td>
    <td><%= link_to 'Show', result %></td>
    <td><%= link_to 'Edit', edit_result_path(result) %></td>
    <td><%= link_to 'Destroy', result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Result', new_result_path %>
